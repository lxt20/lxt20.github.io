/**index.wxss**/
.content-box{width: 100%;display:flex;flex-direction: column;padding: 15px;box-sizing: border-box;}
.content-title{font-size: 15px;color: #333;}
.table-box{width: 100%;display: flex;flex-direction: column;margin-top: 25px;color: #333;}
.tr{width: 100%;display: flex;flex-direction: row;align-items: center;border-bottom: 1px solid #eeeeee;font-size: 14px;color: #333;}
.th{flex: 1;text-align: center;padding-bottom: 10px;}
.td{flex: 1;text-align: center;padding: 10px 0;color: #f00;}
